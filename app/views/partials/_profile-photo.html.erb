<% if user.profile.profile_photo.attached? %>
  <%= image_tag user.profile.profile_photo.variant(resize_to_limit: resize_array), alt: 'Profile Photo', class: 'profile-photo' %>
<% else %>
  <%= image_tag 'profile/default_photo.png', size: "#{resize_array.first}x#{resize_array.last}", alt: 'Profile Photo', class: 'profile-photo' %>
<% end %>
